	-D_REENTRANT_SOURCE -D_BSD_EXTENSION -D_LIMITS_EXTENSION -D_REGEXP_EXTENSION \
	-D_NET_EXTENSION -D_C99_SNPRINTF_EXTENSION -DPLAN9 -DSQLITE_OMIT_LOAD_EXTENSION \
	-DSQLITE_OMIT_TRACE 

shell.$O: 
