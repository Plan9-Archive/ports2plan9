</$objtype/mkfile
LIB=libsqlite3.a$O
OFILES=sqlite3.$O 
HFILES=config.h 

</sys/src/cmd/mklib

CC=pcc
LD=pcc
CFLAGS=-c -I. -B -D_POSIX_SOURCE -D_SUSV2_SOURCE -D_RESEARCH_SOURCE -D_PLAN9_SOURCE \
	-D_REENTRANT_SOURCE -D_BSD_EXTENSION -D_LIMITS_EXTENSION -D_REGEXP_EXTENSION \
	-D_NET_EXTENSION -D_C99_SNPRINTF_EXTENSION -D_HAVE_SQLITE_CONFIG_H -DPLAN9 \
	-DHAVE_STRERROR_R 

sqlite3.$O: 

nuke:V:
	mk clean
	rm -f $LIB
